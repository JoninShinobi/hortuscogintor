{
  "version": 2,
  "builds": [
    {
      "src": "hortus_cognitor/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "hortus_cognitor/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "hortus_cognitor.settings"
  }
}